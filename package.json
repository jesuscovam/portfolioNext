		"validate": "npm-run-all --parallel check-types check-format"
	},
	"devDependencies": {
		"@types/node": "^14.0.1",
		"@types/react": "^16.9.35",
		"husky": "^4.2.5",
		"lint-staged": "^10.2.4",
