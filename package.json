	"private": true,
	"dependencies": {
		"@material-ui/core": "latest",
		"@material-ui/icons": "^4.9.1",
		"clsx": "latest",
		"next": "latest",
		"prop-types": "latest",
		"react": "latest",
		"react-dom": "latest",
		"react-spring": "^8.0.27"
	},
	"scripts": {
